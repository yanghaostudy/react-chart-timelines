{
  "name": "react-chart-timelines",
  "version": "2.7.9",
  "description": "React Chart TimeLines",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib coverage",
    "build:js": "babel src/ -d lib/",
    "watch:js": "babel -w src/ -d lib/",
    "build:css": "node-sass src/scss/style.scss lib/css/style.css",
    "watch:css": "node-sass -w src/scss/style.scss lib/css/style.css",
    "watch": "npm run watch:js & npm run watch:css",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "test": "npm run lint && npm run unit",
    "lint": "npm run lint:prettier && npm run lint:js",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "eslint . --ext .js,.jsx --fix",
    "lint:prettier": "prettier --list-different \"{e2e,src}/**/*.{js,jsx}\"",
    "lint:prettier:fix": "prettier --write \"{e2e,src}/**/*.{js,jsx}\"",
    "unit": "jest",
    "coverage": "jest --coverage --collectCoverageFrom='**/*.{js,jsx}'",
    "prepublish": "npm run clean && npm run build",
    "demo:deploy": "./demo/ops/deploy.sh"
  },
  "keywords": [
    "timeline",
    "schedule",
    "history",
    "react",
    "gantt",
    "horizontal",
    "library",
    "scroll",
    "scss",
    "sass",
    "tracks",
    "time"
  ],
  "repository": "git@github.com:yanghaostudy/react-timelines.git",
  "author": "yhnode",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "files": [
    "src",
    "lib"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.4.0",
    "node-sass": "^4.14.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "16.13.1",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "jest": {
    "rootDir": "./src",
    "resetMocks": true,
    "resetModules": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/utils/raf.js",
      "<rootDir>/utils/events.js",
      "<rootDir>/utils/computedStyle.js",
      "<rootDir>/propTypes.js"
    ],
    "setupFiles": [
      "../jestSetup.js"
    ]
  },
  "resolutions": {
    "**/**/minimist": "1.2.5"
  }
}
